<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="MissingDefaultResource">
    <item>
        <shape>
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"></padding>
            <gradient
                android:type="linear"
                android:angle="0"
                android:endColor="#284180"
                android:startColor="#284180" />
            <corners android:radius="15dp"></corners>

        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape>
            <padding
                android:top="20dp"
                android:left="17dp"
                android:right="10dp"
                android:bottom="20dp"></padding>
            <corners android:radius="10dp"></corners>

            <solid android:color="#fff" />
        </shape>
    </item>

</layer-list>
